#include<stdio.h>
#include<conio.h>
#include<ctype.h>
int main()
{
  char ch;
  printf("Enter any character ");
  scanf("%c",&ch);
  if(isalpha(ch))
  {
  	if(isupper(ch))
  	printf("Lower form is %c\n",tolower(ch));
  	else
  	printf("It is not an upper case alphabet\n");
  }
  else
  printf("it is not an alphabet\n");
  getch();
  return 0;
}
